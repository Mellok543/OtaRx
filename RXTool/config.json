              "freq1": 510,
              "freq2": 2440,
              "rateidx": 8,
              "grid1": 10,
              "grid2": 10,
              "wifi-on-interval": 60,
              "time1": 0,
              "time2": 0,
              "rcvr-uart-baud": 420000,
              "lock-on-first-connection": true,
              "flash-discriminator": 1435200097,
              "is-airport": false,
